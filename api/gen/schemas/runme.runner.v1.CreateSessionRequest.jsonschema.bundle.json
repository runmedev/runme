{
  "$defs": {
    "runme.runner.v1.CreateSessionRequest.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(env_store_type)$": {
          "anyOf": [
            {
              "pattern": "^SESSION_ENV_STORE_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SESSION_ENV_STORE_TYPE_OWL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "optional selection of which env store implementation to sue",
          "title": "Session Env Store Type"
        }
      },
      "properties": {
        "envStoreType": {
          "anyOf": [
            {
              "pattern": "^SESSION_ENV_STORE_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SESSION_ENV_STORE_TYPE_OWL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "optional selection of which env store implementation to sue",
          "title": "Session Env Store Type"
        },
        "envs": {
          "description": "envs field provides an initial set of environment variables\n for a newly created session.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "metadata": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "metadata is a map of client specific metadata.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "project": {
          "$ref": "#/$defs/runme.runner.v1.Project.jsonschema.json",
          "description": "optional project from which to immediately load environment variables"
        }
      },
      "title": "Create Session Request",
      "type": "object"
    },
    "runme.runner.v1.Project.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(env_direnv)$": {
          "anyOf": [
            {
              "pattern": "^DIR_ENV_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "DIR_ENV_ENABLED_WARN",
                "DIR_ENV_ENABLED_ERROR",
                "DIR_ENV_DISABLED"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "specifies if and how direnv should be used",
          "title": "Dir Env"
        },
        "^(env_load_order)$": {
          "description": "list of environment files to try and load\n start with",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "properties": {
        "envDirenv": {
          "anyOf": [
            {
              "pattern": "^DIR_ENV_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "DIR_ENV_ENABLED_WARN",
                "DIR_ENV_ENABLED_ERROR",
                "DIR_ENV_DISABLED"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "specifies if and how direnv should be used",
          "title": "Dir Env"
        },
        "envLoadOrder": {
          "description": "list of environment files to try and load\n start with",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "root": {
          "default": "",
          "description": "project root folder",
          "type": "string"
        }
      },
      "title": "Project",
      "type": "object"
    }
  },
  "$id": "runme.runner.v1.CreateSessionRequest.jsonschema.bundle.json",
  "$ref": "#/$defs/runme.runner.v1.CreateSessionRequest.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
