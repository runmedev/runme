{
  "$id": "runme.parser.v1.Frontmatter.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "category": {
      "description": "Deprecated category in favor of Tag",
      "type": "string"
    },
    "cwd": {
      "type": "string"
    },
    "runme": {
      "$ref": "runme.parser.v1.FrontmatterRunme.schema.strict.json"
    },
    "shell": {
      "type": "string"
    },
    "skip_prompts": {
      "type": "boolean"
    },
    "tag": {
      "type": "string"
    },
    "terminal_rows": {
      "type": "string"
    }
  },
  "required": [
    "shell",
    "cwd",
    "skip_prompts",
    "category",
    "terminal_rows",
    "tag"
  ],
  "title": "Frontmatter",
  "type": "object"
}
