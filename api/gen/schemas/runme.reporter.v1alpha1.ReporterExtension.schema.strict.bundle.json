{
  "$defs": {
    "runme.reporter.v1alpha1.ReporterDevice.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "arch": {
          "type": "string"
        },
        "hostname": {
          "type": "string"
        },
        "mac_address": {
          "type": "string"
        },
        "platform": {
          "type": "string"
        },
        "release": {
          "type": "string"
        },
        "shell": {
          "type": "string"
        },
        "vendor": {
          "type": "string"
        },
        "vs_app_host": {
          "type": "string"
        },
        "vs_app_name": {
          "type": "string"
        },
        "vs_app_session_id": {
          "type": "string"
        },
        "vs_machine_id": {
          "type": "string"
        },
        "vs_metadata": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "required": [
        "mac_address",
        "hostname",
        "platform",
        "release",
        "arch",
        "vendor",
        "shell",
        "vs_app_host",
        "vs_app_name",
        "vs_app_session_id",
        "vs_machine_id"
      ],
      "title": "Reporter Device",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterExtension.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "auto_save": {
          "type": "boolean"
        },
        "device": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterDevice.schema.strict.json"
        },
        "file": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterFile.schema.strict.json"
        },
        "git": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterGit.schema.strict.json"
        },
        "session": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterSession.schema.strict.json"
        }
      },
      "required": [
        "auto_save"
      ],
      "title": "Reporter Extension",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterFile.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "content": {
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "path": {
          "type": "string"
        }
      },
      "required": [
        "path",
        "content"
      ],
      "title": "Reporter File",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterGit.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "branch": {
          "type": "string"
        },
        "commit": {
          "type": "string"
        },
        "repository": {
          "type": "string"
        }
      },
      "required": [
        "repository",
        "branch",
        "commit"
      ],
      "title": "Reporter Git",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterSession.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "masked_output": {
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "plain_output": {
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        }
      },
      "required": [
        "plain_output",
        "masked_output"
      ],
      "title": "Reporter Session",
      "type": "object"
    }
  },
  "$id": "runme.reporter.v1alpha1.ReporterExtension.schema.strict.bundle.json",
  "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterExtension.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
