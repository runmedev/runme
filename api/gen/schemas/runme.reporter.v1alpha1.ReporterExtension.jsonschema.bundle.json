        },
        "repository": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Reporter Git",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterSession.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(masked_output)$": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "^(plain_output)$": {
          "default": null,
