    "runme.reporter.v1alpha1.ReporterExtension.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(auto_save)$": {
          "default": false,
          "type": "boolean"
