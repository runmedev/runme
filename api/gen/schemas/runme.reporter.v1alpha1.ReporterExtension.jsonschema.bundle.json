        "platform": {
          "default": "",
          "type": "string"
        },
        "release": {
          "default": "",
          "type": "string"
        },
        "shell": {
          "default": "",
          "type": "string"
        },
        "vendor": {
          "default": "",
          "type": "string"
        },
        "vsAppHost": {
          "default": "",
          "type": "string"
