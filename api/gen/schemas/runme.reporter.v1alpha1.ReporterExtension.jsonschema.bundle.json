        "^(vs_app_name)$": {
          "default": "",
          "type": "string"
        },
        "^(vs_app_session_id)$": {
          "default": "",
          "type": "string"
        },
        "^(vs_machine_id)$": {
          "default": "",
          "type": "string"
        },
        "^(vs_metadata)$": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "properties": {
        "arch": {
          "default": "",
          "type": "string"
