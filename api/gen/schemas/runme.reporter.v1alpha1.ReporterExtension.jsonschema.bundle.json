      "properties": {
        "autoSave": {
          "default": false,
          "type": "boolean"
        },
        "device": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterDevice.jsonschema.json"
        },
        "file": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterFile.jsonschema.json"
        },
        "git": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterGit.jsonschema.json"
        },
        "session": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterSession.jsonschema.json"
        }
      },
      "title": "Reporter Extension",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterFile.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "content": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
