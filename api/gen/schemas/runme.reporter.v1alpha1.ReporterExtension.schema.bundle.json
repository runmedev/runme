{
  "$defs": {
    "runme.reporter.v1alpha1.ReporterDevice.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(macAddress)$": {
          "default": "",
          "type": "string"
        },
        "^(vsAppHost)$": {
          "default": "",
          "type": "string"
        },
        "^(vsAppName)$": {
          "default": "",
          "type": "string"
        },
        "^(vsAppSessionId)$": {
          "default": "",
          "type": "string"
        },
        "^(vsMachineId)$": {
          "default": "",
          "type": "string"
        },
        "^(vsMetadata)$": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "properties": {
        "arch": {
          "default": "",
          "type": "string"
        },
        "hostname": {
          "default": "",
          "type": "string"
        },
        "mac_address": {
          "default": "",
          "type": "string"
        },
        "platform": {
          "default": "",
          "type": "string"
        },
        "release": {
          "default": "",
          "type": "string"
        },
        "shell": {
          "default": "",
          "type": "string"
        },
        "vendor": {
          "default": "",
          "type": "string"
        },
        "vs_app_host": {
          "default": "",
          "type": "string"
        },
        "vs_app_name": {
          "default": "",
          "type": "string"
        },
        "vs_app_session_id": {
          "default": "",
          "type": "string"
        },
        "vs_machine_id": {
          "default": "",
          "type": "string"
        },
        "vs_metadata": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "title": "Reporter Device",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterExtension.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(autoSave)$": {
          "default": false,
          "type": "boolean"
        }
      },
      "properties": {
        "auto_save": {
          "default": false,
          "type": "boolean"
        },
        "device": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterDevice.schema.json"
        },
        "file": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterFile.schema.json"
        },
        "git": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterGit.schema.json"
        },
        "session": {
          "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterSession.schema.json"
        }
      },
      "title": "Reporter Extension",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterFile.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "content": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "path": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Reporter File",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterGit.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "branch": {
          "default": "",
          "type": "string"
        },
        "commit": {
          "default": "",
          "type": "string"
        },
        "repository": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Reporter Git",
      "type": "object"
    },
    "runme.reporter.v1alpha1.ReporterSession.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(maskedOutput)$": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "^(plainOutput)$": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        }
      },
      "properties": {
        "masked_output": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "plain_output": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        }
      },
      "title": "Reporter Session",
      "type": "object"
    }
  },
  "$id": "runme.reporter.v1alpha1.ReporterExtension.schema.bundle.json",
  "$ref": "#/$defs/runme.reporter.v1alpha1.ReporterExtension.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
