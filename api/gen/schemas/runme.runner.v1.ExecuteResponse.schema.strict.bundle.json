{
  "$defs": {
    "google.protobuf.UInt32Value.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "description": "The uint32 value.",
      "maximum": 4294967295,
      "minimum": 0,
      "title": "U Int32 Value",
      "type": "integer"
    },
    "runme.runner.v1.ExecuteResponse.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "exit_code": {
          "$ref": "#/$defs/google.protobuf.UInt32Value.schema.strict.json",
          "description": "exit_code is sent only in the final message."
        },
        "mime_type": {
          "description": "This is only sent once in the first response containing stdout_data.",
          "title": "mime_type is a detected MIME type of the stdout_data.",
          "type": "string"
        },
        "pid": {
          "$ref": "#/$defs/runme.runner.v1.ProcessPID.schema.strict.json",
          "description": "pid contains the process' PID\n this is only sent once in an initial response for background processes."
        },
        "stderr_data": {
          "description": "stderr_data contains bytes from stderr since the last response.",
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "stdout_data": {
          "description": "stdout_data contains bytes from stdout since the last response.",
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        }
      },
      "required": [
        "stdout_data",
        "stderr_data",
        "mime_type"
      ],
      "title": "Execute Response",
      "type": "object"
    },
    "runme.runner.v1.ProcessPID.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "pid": {
          "type": "integer"
        }
      },
      "required": [
        "pid"
      ],
      "title": "ProcessPID",
      "type": "object"
    }
  },
  "$id": "runme.runner.v1.ExecuteResponse.schema.strict.bundle.json",
  "$ref": "#/$defs/runme.runner.v1.ExecuteResponse.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
