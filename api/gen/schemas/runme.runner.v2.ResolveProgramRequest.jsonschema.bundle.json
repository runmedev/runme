{
  "$defs": {
    "runme.runner.v2.Project.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(env_load_order)$": {
          "description": "env_load_order is list of environment files\n to try and load env from.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "properties": {
        "envLoadOrder": {
          "description": "env_load_order is list of environment files\n to try and load env from.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "root": {
          "default": "",
          "description": "root is a root directory of the project.\n The semantic is the same as for the \"--project\"\n flag in \"runme\".",
          "type": "string"
        }
      },
      "title": "Project",
      "type": "object"
    },
    "runme.runner.v2.ResolveProgramCommandList.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "lines": {
          "description": "commands are commands to be executed by the program.\n The commands are joined and executed as a script.\n For example: [\"echo 'Hello, World'\", \"ls -l /etc\"].",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Resolve Program Command List",
      "type": "object"
    },
    "runme.runner.v2.ResolveProgramRequest.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(language_id)$": {
          "default": "",
          "description": "language id associated with script.",
          "type": "string"
        },
        "^(session_id)$": {
          "default": "",
          "description": "session_id indicates which session is the source of\n environment variables. If not provided, the most recent\n session can be used using session_strategy.",
          "type": "string"
        },
        "^(session_strategy)$": {
          "anyOf": [
            {
              "pattern": "^SESSION_STRATEGY_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SESSION_STRATEGY_MOST_RECENT"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "session_strategy is a strategy for selecting the session.",
          "title": "Session Strategy"
        }
      },
      "properties": {
        "commands": {
          "$ref": "#/$defs/runme.runner.v2.ResolveProgramCommandList.jsonschema.json",
          "description": "commands are commands to be executed by the program.\n The commands are joined and executed as a script."
        },
        "env": {
          "description": "env is a list of explicit environment variables that will be used\n to resolve the environment variables found in the source.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "languageId": {
          "default": "",
          "description": "language id associated with script.",
          "type": "string"
        },
        "mode": {
          "anyOf": [
            {
              "pattern": "^MODE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "MODE_PROMPT_ALL",
                "MODE_SKIP_ALL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "mode determines how variables resolution occurs.\n It is usually based on document or cell annotation config.",
          "title": "Mode"
        },
        "project": {
          "$ref": "#/$defs/runme.runner.v2.Project.jsonschema.json",
          "description": "project used to load environment variables from .env files."
        },
        "retention": {
          "anyOf": [
            {
              "pattern": "^RETENTION_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "RETENTION_FIRST_RUN",
                "RETENTION_LAST_RUN"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "retention determines how variables are retained once resolved.",
          "title": "Retention"
        },
        "script": {
          "description": "script is code to be executed by the program.\n Individual lines are joined with the new line character.",
          "type": "string"
        },
        "sessionId": {
          "default": "",
          "description": "session_id indicates which session is the source of\n environment variables. If not provided, the most recent\n session can be used using session_strategy.",
          "type": "string"
        },
        "sessionStrategy": {
          "anyOf": [
            {
              "pattern": "^SESSION_STRATEGY_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SESSION_STRATEGY_MOST_RECENT"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "session_strategy is a strategy for selecting the session.",
          "title": "Session Strategy"
        }
      },
      "title": "Resolve Program Request",
      "type": "object"
    }
  },
  "$id": "runme.runner.v2.ResolveProgramRequest.jsonschema.bundle.json",
  "$ref": "#/$defs/runme.runner.v2.ResolveProgramRequest.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
