{
  "$id": "runme.reporter.v1alpha1.ReporterSession.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "masked_output": {
      "pattern": "^[A-Za-z0-9+/]*={0,2}$",
      "type": "string"
    },
    "plain_output": {
      "pattern": "^[A-Za-z0-9+/]*={0,2}$",
      "type": "string"
    }
  },
  "required": [
    "plain_output",
    "masked_output"
  ],
  "title": "Reporter Session",
  "type": "object"
}
