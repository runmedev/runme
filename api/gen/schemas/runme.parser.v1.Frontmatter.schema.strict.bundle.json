{
  "$defs": {
    "runme.parser.v1.Frontmatter.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "category": {
          "description": "Deprecated category in favor of Tag",
          "type": "string"
        },
        "cwd": {
          "type": "string"
        },
        "runme": {
          "$ref": "#/$defs/runme.parser.v1.FrontmatterRunme.schema.strict.json"
        },
        "shell": {
          "type": "string"
        },
        "skip_prompts": {
          "type": "boolean"
        },
        "tag": {
          "type": "string"
        },
        "terminal_rows": {
          "type": "string"
        }
      },
      "required": [
        "shell",
        "cwd",
        "skip_prompts",
        "category",
        "terminal_rows",
        "tag"
      ],
      "title": "Frontmatter",
      "type": "object"
    },
    "runme.parser.v1.FrontmatterRunme.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "session": {
          "$ref": "#/$defs/runme.parser.v1.RunmeSession.schema.strict.json"
        },
        "version": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "version"
      ],
      "title": "Frontmatter Runme",
      "type": "object"
    },
    "runme.parser.v1.RunmeSession.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "document": {
          "$ref": "#/$defs/runme.parser.v1.RunmeSessionDocument.schema.strict.json"
        },
        "id": {
          "type": "string"
        }
      },
      "required": [
        "id"
      ],
      "title": "Runme Session",
      "type": "object"
    },
    "runme.parser.v1.RunmeSessionDocument.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "relative_path": {
          "type": "string"
        }
      },
      "required": [
        "relative_path"
      ],
      "title": "Runme Session Document",
      "type": "object"
    }
  },
  "$id": "runme.parser.v1.Frontmatter.schema.strict.bundle.json",
  "$ref": "#/$defs/runme.parser.v1.Frontmatter.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
