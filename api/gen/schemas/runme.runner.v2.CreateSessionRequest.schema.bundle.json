{
  "$defs": {
    "runme.runner.v2.CreateSessionRequest.Config.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(envStoreSeeding)$": {
          "anyOf": [
            {
              "enum": [
                "SESSION_ENV_STORE_SEEDING_UNSPECIFIED",
                "SESSION_ENV_STORE_SEEDING_SYSTEM"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "description": "how to seed initial ENV",
          "title": "Session Env Store Seeding"
        },
        "^(envStoreType)$": {
          "anyOf": [
            {
              "enum": [
                "SESSION_ENV_STORE_TYPE_UNSPECIFIED",
                "SESSION_ENV_STORE_TYPE_OWL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "description": "optional selection of which env store implementation to use.",
          "title": "Session Env Store Type"
        }
      },
      "properties": {
        "env_store_seeding": {
          "anyOf": [
            {
              "enum": [
                "SESSION_ENV_STORE_SEEDING_UNSPECIFIED",
                "SESSION_ENV_STORE_SEEDING_SYSTEM"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "description": "how to seed initial ENV",
          "title": "Session Env Store Seeding"
        },
        "env_store_type": {
          "anyOf": [
            {
              "enum": [
                "SESSION_ENV_STORE_TYPE_UNSPECIFIED",
                "SESSION_ENV_STORE_TYPE_OWL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "description": "optional selection of which env store implementation to use.",
          "title": "Session Env Store Type"
        }
      },
      "title": "Config",
      "type": "object"
    },
    "runme.runner.v2.CreateSessionRequest.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(envStoreType)$": {
          "anyOf": [
            {
              "enum": [
                "SESSION_ENV_STORE_TYPE_UNSPECIFIED",
                "SESSION_ENV_STORE_TYPE_OWL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "description": "Deprecated use config instead. optional selection\n of which env store implementation to use.",
          "title": "Session Env Store Type"
        }
      },
      "properties": {
        "config": {
          "$ref": "#/$defs/runme.runner.v2.CreateSessionRequest.Config.schema.json"
        },
        "env": {
          "description": "env field provides an initial set of environment variables\n for a newly created session.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "env_store_type": {
          "anyOf": [
            {
              "enum": [
                "SESSION_ENV_STORE_TYPE_UNSPECIFIED",
                "SESSION_ENV_STORE_TYPE_OWL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "description": "Deprecated use config instead. optional selection\n of which env store implementation to use.",
          "title": "Session Env Store Type"
        },
        "metadata": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "metadata is a map of client specific metadata.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "project": {
          "$ref": "#/$defs/runme.runner.v2.Project.schema.json",
          "description": "project from which to load environment variables.\n They will be appended to the list from the env field.\n The env field has a higher priority."
        }
      },
      "title": "Create Session Request",
      "type": "object"
    },
    "runme.runner.v2.Project.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(envLoadOrder)$": {
          "description": "env_load_order is list of environment files\n to try and load env from.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "properties": {
        "env_load_order": {
          "description": "env_load_order is list of environment files\n to try and load env from.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "root": {
          "default": "",
          "description": "root is a root directory of the project.\n The semantic is the same as for the \"--project\"\n flag in \"runme\".",
          "type": "string"
        }
      },
      "title": "Project",
      "type": "object"
    }
  },
  "$id": "runme.runner.v2.CreateSessionRequest.schema.bundle.json",
  "$ref": "#/$defs/runme.runner.v2.CreateSessionRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
