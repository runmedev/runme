{
  "$defs": {
    "runme.runner.v1.Winsize.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "cols": {
          "anyOf": [
            {
              "maximum": 4294967295,
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "number of columns (in cells)"
        },
        "rows": {
          "anyOf": [
            {
              "maximum": 4294967295,
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "number of rows (in cells)"
        },
        "x": {
          "anyOf": [
            {
              "maximum": 4294967295,
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "width in pixels"
        },
        "y": {
          "anyOf": [
            {
              "maximum": 4294967295,
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "height in pixels"
        }
      },
      "title": "Winsize",
      "type": "object"
    }
  },
  "$id": "runme.runner.v1.Winsize.schema.bundle.json",
  "$ref": "#/$defs/runme.runner.v1.Winsize.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
